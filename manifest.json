{
  "name": "Ping.",
  "description" : "Ping a website from the Chrome Omnibox. Simple type 'ping' and hit tab followed by the domain.",
  "version": "0.3",
  "background": {
    "page": "background.html"
  },
  "omnibox": { "keyword" : "ping" },
  "manifest_version": 2,
  "permissions": [
  	"http://*.scottymeuk.co.uk/",
  	"notifications",
    "tabs"
  ],
  "web_accessible_resources": [
    "Chrome.png",
    "Power.png"
  ],
  "icons": { 
    "16": "icon16.png",
    "48": "icon48.png",
    "64": "icon64.png",
    "128": "icon128.png" 
  }
}
