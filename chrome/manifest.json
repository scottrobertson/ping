{
  "name": "Ping.",
  "description" : "Ping a website from the Chrome Omnibox. Simple type 'ping' and hit tab followed by the domain.",
  "version": "1.0.1",
  "background": {
    "page": "background.html"
  },
  "omnibox": {
    "keyword" : "ping"
  },
  "manifest_version": 2,
  "permissions": [
  	"http://scottymeuk-ping.herokuapp.com/",
  	"notifications",
    "tabs"
  ],
  "web_accessible_resources": [
    "/img/Chrome.png",
    "/img/Power.png"
  ],
  "icons": {
    "16": "/img/icon16.png",
    "48": "/img/icon48.png",
    "64": "/img/icon64.png",
    "128": "/img/icon128.png"
  }
}
